{
  "name": "@fastcms/eslint-config",
  "version": "1.7.0",
  "private": false,
  "description": "Shared @eslint configs for web development projects of @fastcms.",
  "keywords": [
    "babel",
    "eslint",
    "eslint-config",
    "fastcms",
    "linter",
    "markdown",
    "miniprogram",
    "prettier",
    "react",
    "typescript"
  ],
  "homepage": "https://npm.im/@fastcms/eslint-config",
  "bugs": {
    "url": "https://github.com/fastcms/eslint-config/issues",
    "email": "hello@fastcms.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fastcms/eslint-config"
  },
  "author": {
    "name": "Ritchie Zhu",
    "email": "laozhu.me@gmail.com",
    "url": "https://laozhu.me"
  },
  "contributors": [
    {
      "name": "Ritchie Zhu",
      "email": "laozhu.me@gmail.com",
      "url": "https://laozhu.me"
    }
  ],
  "main": "index.js",
  "files": [
    "base.js",
    "index.js",
    "lib/*",
    "util/*"
  ],
  "scripts": {
    "ci": "run-s prettier eslint test:ci",
    "clean": "rimraf .eslintcache coverage",
    "commit": "git-cz",
    "commit:dirty": "git-cz --no-verify",
    "eslint": "eslint --cache --ext .js,.mjs,.jsx,.ts,.tsx,.md,.mdx .",
    "eslint:fix": "eslint --cache --fix --ext .js,.mjs,.jsx,.ts,.tsx,.md,.mdx .",
    "prepublishOnly": "yarn run ci",
    "prettier": "prettier --check \"**/*.{json,html,md,mdx}\"",
    "prettier:fix": "prettier --write \"**/*.{json,html,md,mdx}\"",
    "test": "jest --cache --passWithNoTests  --watch",
    "test:ci": "jest --ci --passWithNoTests"
  },
  "lint-staged": {
    "**/*.{js,mjs,jsx,ts,tsx,md,mdx}": "yarn run eslint",
    "**/*.{json,html,md,mdx}": "yarn run prettier"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "browserslist": [
    "last 1 chrome version",
    "last 1 edge version",
    "last 1 firefox version",
    "last 1 safari version",
    "last 1 ios version",
    "last 1 android version"
  ],
  "prettier": {
    "printWidth": 100,
    "quoteProps": "consistent",
    "singleQuote": true,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "extends": "./index.js",
    "root": true
  },
  "eslintIgnore": [
    "__files__",
    "coverage",
    "node_modules"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "**/*.ts",
      "!**/*.d.ts",
      "!__files__/**",
      "!coverage/**",
      "!node_modules/**"
    ],
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "@swc/jest"
    },
    "verbose": true
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "eslint-plugin-unicorn": "^37.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@swc/core": "^1.2.100",
    "@swc/jest": "^0.2.4",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/eslint": "^7.28.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "commitizen": "^4.2.4",
    "cz-emoji": "^1.3.1",
    "eslint": "^7.32.0",
    "husky": "^7.0.2",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "prettier-plugin-packagejson": "^2.2.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "@babel/core": "^7.15.8",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org",
    "tag": "latest"
  }
}
